{
  "name": "8fold/ef-laravel-dms-universals",
  "description": "Universal helper files for 8fold sites based on Laravel using the custom Document Management System",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Josh Bruce",
      "email": "josh@8fold.pro"
    }
  ],
  "require": {
    "php": "~7|^8.0",
    "8fold/php-foldable": "^1.3",
    "8fold/php-shoop": "^0.10.17",
    "8fold/php-shoop-shelf": "^0.8.0",
    "8fold/laravel-markup": "^0.0.50",
    "jaybizzle/crawler-detect": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.1",
    "orchestra/testbench": "^6.6",
    "orchestra/testbench-browser-kit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Eightfold\\DmsHelpers\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\DmsHelpers\\Tests\\": "./tests",
      "Eightfold\\Foldable\\Tests\\": "./vendor/8fold/php-foldable/tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}
